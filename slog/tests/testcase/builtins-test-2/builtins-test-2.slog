(g-foo 1 1)
(g-foo 2 2)
(g-foo 3 3)

[(g-foo x y)
 (/= x y)
 -->
 (g-empty x y)]

[(g-foo x y)
 (+ x y 2)
 -->
 (g-one x y)]

(g-one-expected 1 1)
[(g-one x y) (g-one-expected x y) --> (g-one-checked x y)]

