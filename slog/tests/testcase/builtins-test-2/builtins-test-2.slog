(i-nums 0 1 2 3 4 5)

[(i-nums n0 n1 n2 n3 n4 n5)
 -->
 [(+ x n2 n3) --> (i-res1 x)]
 [(- n5 x n3) --> (i-res2 x)]
 [(+ n0 x n3) --> (i-res3 x)]
 [(add1 x n5) --> (i-res4 x)]
 ]

(i-res1-expected 1)
(i-res2-expected 2)
(i-res3-expected 3)
(i-res4-expected 4)

[(i-res1-expected x) (i-res1 x) --> (i-res1-checked x)]
[(i-res2-expected x) (i-res2 x) --> (i-res2-checked x)]
[(i-res3-expected x) (i-res3 x) --> (i-res3-checked x)]
[(i-res4-expected x) (i-res4 x) --> (i-res4-checked x)]


