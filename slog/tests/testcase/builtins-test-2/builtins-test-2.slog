(e-seed 1)

[(e-seed x)
 (< 0 x) (<= x 1) 
 -->
 (e-foo x {add1 x} {+ x 2})
 (e-seed {* x 10})]

(e-foo 1 2 3)
(e-foo 10 20 30)
(e-foo 11 12 13)
(e-foo 21 22 23)

[(e-foo x y z)
 (+ x y z)
 (- z y x)
 (- z x y)
 (< x y)
 (< y z)
 (<= x z)
 (>= z y)
 (> z x)
 -->
 (e-res x y z)]

(e-expected 1 2 3)
(e-expected 10 20 30)

[(e-res x y z) (e-expected x y z) --> (e-checked x y z)]

