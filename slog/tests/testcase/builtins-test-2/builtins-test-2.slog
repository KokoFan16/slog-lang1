(f-foo 1 2 3)
(f-foo 2 3 4)
(f-foo 3 4 5)
(f-foo 4 5 6)
(f-foo 5 6 7)

[(f-foo x y z)
 (< 0 x) (< x 6)
 -->
 (f-seed {- x 0})
 (f-seed {+ x 0})
 (f-seed {* 1 {+ 4 {- x {+ 2 2}}}})]

[(f-seed x)
 -->
 ;; the following two clauses must do the same thing:
 (f-foo x {add1 x} {+ x 2})
 (f-foo {+ x {- 0 {- x x}}} {sub1 {- {add1 {+ x {add1 x}}} x}} {+ {- 2 x} {+ x x}})]

